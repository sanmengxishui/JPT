        <Button Content="Standard Resolution" HorizontalAlignment="Left" Height="54" Margin="10,100,0,0" VerticalAlignment="Top" Width="290" Click="Button_Click_3" FontSize="24" MouseDown="Button_MouseDown_2" TouchDown="Button_TouchDown_1"/>
